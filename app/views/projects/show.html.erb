<div class="container header" style="margin-top: 100px;">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-md-4">
      <img id="logo" src="data:image/png;base64," height="75">
    </div>
    <div class="col-xs-12 col-sm-6 col-md-8">
      <hr>
    </div>
  </div>
</div>
<div class="container">
  <div class="row">
    <div class="col-xs-12">
  <hr>
  </div>
  </div>
  <div class="row">
    <div class="col-md-9 col-sm-8 product-display">
      <div class="col-lg-6 product-preview">
        <img id="product-image" src="https://1.bp.blogspot.com/-fCJYZnIHYBo/WpEGyT8Bx2I/AAAAAAAAPME/a32q0EriMqgQO-is3YA-bb2yFjbBmJubgCLcBGAs/s1600/EN%2Brural%2BEl%2BPais.jpg">
      </div>
      <div class="col-lg-6 product-data">
        <h3><%=@project["name"] %></h3>
        <hr>
        <div class="product-details">
          <h5 id="logo-header">Proyecto</h5>
          <p><%=@project["description"] %></p>
        </div>
      </div>
    </div>
    <div class="col-md-3 col-sm-4 product-options well">
      <h4 id="logo-header">Objetivos de desarrollo sostenible</h4>
      <div id="logo-display">
        <img src="data:image/jpeg;base64," alt="">
      </div>
      <hr>
      <%= link_to "Financiar", finance_project_path(@project["id"]), class:"btn btn-primary btn-block" %>
    </div>
  </div>
</div>
</div>
